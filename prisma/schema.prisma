generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Section {
  id              Int       @id @default(autoincrement())
  ClassCapacity   Int?
  ClassNumber     Int?
  ClassTitle      String?
  Component       String?
  CourseNumber    String
  EndDate         DateTime?
  EndTime         DateTime?
  Friday          Boolean?
  InstructionMode String?
  InstructorFirst String?
  InstructorLast  String?
  Location        String?
  Monday          Boolean?
  Saturday        Boolean?
  Section         String
  StartDate       DateTime?
  StartTime       DateTime?
  Subject         String
  Sunday          Boolean?
  Term            String?
  Thursday        Boolean?
  Tuesday         Boolean?
  TotalEnrollment Int?
  Units           Float?
  Wednesday       Boolean?
  A               Int?
  Am              Int?
  Bp              Int?
  B               Int?
  Bm              Int?
  Cp              Int?
  C               Int?
  Cm              Int?
  Dp              Int?
  D               Int?
  Dm              Int?
  F               Int?

  @@unique([Subject, CourseNumber, Section], name: "sectionConstraint")
  @@map("sections")
}
